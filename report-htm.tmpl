<P class="p0"><FONT class="ft0">Couchbase Cluster Health Check Report</FONT></P>
<P class="p1"><FONT class="ft1">Tool Version: $globals['versions']</FONT></P>
<P class="p0"><FONT class="ft1">Execution Time: $globals['report_time'].strftime("%Y-%m-%d %H:%M:%S")</FONT></P>
#if $globals.cluster_health == "Error"
<P class="p2"><FONT class="ft2">Overall cluster health: </FONT><FONT class="ft30">Immediate action needed</FONT></P>
#elif $globals.cluster_health == "Warning"
<P class="p2"><FONT class="ft2">Overall cluster health: </FONT><FONT class="ft31">Attention needed</FONT></P>
#else
<P class="p2"><FONT class="ft2">Overall cluster health: </FONT><FONT class="ft32">OK</FONT></P>
#end if
#if $indicator_error_exist
<P class="p3"><FONT class="ft4">Couchbase &#8211; Alerts</FONT></P>
<P class="p4"><FONT class="ft5">Cluster-wide metrics</FONT></P>
#for $counter in sorted($indicator_error.keys())
#set $error_list = $indicator_error[$counter]
#for $error_values in $error_list:
<P class="p20"><FONT class="ft3">$error_values["description"]</FONT></P>
<TABLE cellpadding=0 cellspacing=0 class="t0">
<TR class="tr0">
	<TD class="td0">&#8226;</TD>
	<TD class="td1"><FONT class="ft6">Symptom</FONT></TD>
	<TD class="td2"><FONT class="ft8">&nbsp;</FONT></TD>
</TR>
#for $err_val in sorted($error_values["value"])
<TR class="tr1">
	<TD class="td0">&nbsp;</TD>
	<TD class="td3" colspan=2><FONT class="ft6">Bucket : $error_values["bucket"] , </FONT>
	<FONT class="ft9">Node : $err_val["node"]</FONT></TD>
</TR>
<TR class="tr1">
	<TD class="td0">&nbsp;</TD>
	<TD class="td3"><FONT class="ft6">Value :  </FONT></TD>
	<TD class="td4"><FONT class="ft9"> $err_val["value"]</FONT></TD>
</TR>
#end for
<TR class="tr0">
	<TD class="td0">&#8226;</TD>
	<TD class="td1"><FONT class="ft6">Causes -</FONT></TD>
	<TD class="td2"><FONT class="ft9">$error_values["cause"]</FONT></TD>
</TR>
<TR class="tr0">
	<TD class="td0">&#8226;</TD>
	<TD class="td1"><FONT class="ft6">Impact -</FONT></TD>
	<TD class="td2"><FONT class="ft9">$error_values["impact"]</FONT></TD>
</TR>
<TR class="tr0">
	<TD class="td0">&#8226;</TD>
	<TD class="td1"><FONT class="ft6">Action -</FONT></TD>
	<TD class="td2"><FONT class="ft9">$error_values["action"]</FONT></TD>
</TR>
</TABLE>
#end for
#end for
#end if
