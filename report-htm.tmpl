<TR class="tr6">
	<TD class="td5"><FONT class="ft15">Node IP</FONT></TD>
	<TD class="td6"><FONT class="ft15">Couchbase Server Version</FONT></TD>
	<TD class="td7"><FONT class="ft15">Status</FONT></TD>
</TR>
#for $node in sorted($node_list["nodeList"]["value"])
<TR class="tr6">
	<TD class="td8">$node["host"]</TD>
	<TD class="td9">$node["version"]</TD>
	<TD class="td10">$node["status"]</TD>
</TR>
#end for
</TABLE>
<P class="p8"><FONT class="ft16">&#8226; </FONT><FONT class="ft17">Total number of nodes in the cluster: $node_list["numNodes"]["value"] </FONT></P>
<P class="p8"><FONT class="ft16">&#8226; </FONT><FONT class="ft17">Number of down nodes: $node_list["numDownNodes"]["value"] </FONT></P>
<P class="p8"><FONT class="ft16">&#8226; </FONT><FONT class="ft17">Number of warming up nodes: $node_list["numWarmupNodes"]["value"] </FONT></P>
<P class="p11"><FONT class="ft5">Cluster-wide metrics</FONT></P>
<TABLE cellpadding=0 cellspacing=0 class="t1">
#for $key in sorted($cluster_symptoms.keys())
#set $value = $cluster_symptoms[$key]
<TR class="tr4">
	<TD class="td11">$value["description"]</TD>
	<TD class="td12">$value["value"]</TD>
</TR>
#end for
</TABLE>
<P class="p11"><FONT class="ft5">Bucket metrics</FONT></P>
#for $bucket in sorted($bucket_list.keys())
#set $status = $bucket_list[$bucket]
